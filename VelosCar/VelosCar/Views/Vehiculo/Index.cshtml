@model IEnumerable<VelosCar.Models.Vehiculo>

<div class="table-responsive">
    <table class="table table-hover">
        <thead>
        <tr>
            <th>#</th>
            <th>Marca</th>
            <th>Modelo</th>
            <th>Tipo</th>
            <th>Tarifa</th>
            <th>Opciones</th>
        </tr>
        </thead>
        <tbody id="myTable">
            <tr>
                    <td></td>
                    <td>Chevrolet</td>
                    <td>Swing</td>
                    <td>Hashback</td>
                    <td>$99.999</td>
                    <td>
                        <a title="Detalle" href="#"><span class="glyphicon glyphicon-eye-open"></span></a>
                    </td>
                </tr>
            @foreach (var item in Model)
            {
                <tr>
                    <td></td>
                    <td>@item.Marca</td>
                    <td>@item.Modelo</td>
                    <td>@item.TipoVehiculo.Nombre</td>
                    <td>$@item.Tarifa.Valor</td>
                    <td>
                        <a title="Detalle" href="#"><span class="glyphicon glyphicon-eye-open"></span></a>
                    </td>
                </tr>
            }               
        </tbody>
    </table>
</div>